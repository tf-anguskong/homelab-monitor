    {
      "id": 1,
      "type": "stat",
      "title": "Total Net Worth",
      "description": "Sum of all account balances (assets minus credit owed)",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 10000 },
              { "color": "green", "value": 50000 }
            ]
          },
          "unit": "currencyUSD",
          "decimals": 0,
          "mappings": []
        }
      },
      "datasource": { "type": "influxdb", "uid": "influxdb" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "influxdb", "uid": "influxdb" },
          "query": "from(bucket: \"powermon\")\n  |> range(start: -2h)\n  |> filter(fn: (r) => r._measurement == \"account_balance\" and r._field == \"balance\")\n  |> group(columns: [\"account_name\", \"institution\"])\n  |> last()\n  |> group()\n  |> sum()"
        }
      ]
    },

